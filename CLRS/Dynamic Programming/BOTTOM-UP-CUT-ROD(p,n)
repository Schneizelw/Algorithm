//!BOTTOM-UP-CUT-ROD(p,n)
/*
1   let r[0..n] be a new array
2   r[0] = 0
3   for j = 1 to n
4       q = -ยกร
5       for i = 1 to j
6           q = max(q,p[i]+r[j-i])
7       r[j] = q
8   return r[n]
*/

#include<iostream>
#include<limits>
#include<vector>

using namespace std;

int BOTTOM_UP_CUT_ROD(vector<int> &p,int n)
{
    vector<int> r(n+1,0);
    for(int j=1;j<=n;++j){
        int q = INT_MIN;
        for(int i=1;i<=j;++i){
            int t = p[i]+r[j-i];
            q = q>t?q:t;
        }
        r[j] = q;
    }
    return r[n];
}


int main()
{
    vector<int> p = {0,1,5,8,9,10,17,17,20,24,30};
    cout << BOTTOM_UP_CUT_ROD(p,9);
}
